#
# RPI-WIFI-CONF Autogenerated File
#

auto lo
iface lo inet loopback

auto eth0
iface eth0 inet {{ lan_type }}
  address {{ lan_ip }}
  netmask {{ lan_mask }}
  gateway {{ lan_gateway }}
  broadcast {{ lan_broadcast }}
  nameserver {{ lan_dns }}

auto wlan0
iface wlan0 inet dhcp
  wpa-ssid "{{ wifi_ssid }}"
  wpa-psk  "{{ wifi_passcode }}"
